{
  "name": "p2p chatterbox",
  "description" : "A p2p messaging application built on IPFS and libp2p",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "notifications"
  ]
}
